<?xml version="1.0" encoding="UTF-8"?>
<!-- vim:fdl=1:
-->
<templates id="template" xml:space="preserve">   

<div t-name="log_info_template" class="bs3">
    <div class="container">
        <div class="row">
            <nav class="navbar navbar-default" role="navigation">
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                  <li>
                    <a id="war" href="#">
                        <span class="pull-right">Warnings</span>
                    </a>
                  </li>
                  <li>
                      <span class="badge navbar-text pull-left">{{ len(warnings) }}</span>
                  </li>
                  <li>
                    <a id="err" href="#">
                      <span class="pull-right">Errors</span>
                    </a>
                  </li>
                  <li>
                      <span class="badge navbar-text pull-left">{{ len(errors) }}</span>
                  </li>
                  <li>
                    <a id="in" href="#">
                      <span class="pull-right">Infos</span>
                    </a>
                  </li>
                  <li>
                      <span class="badge navbar-text pull-left">{{ len(infos) }}</span>
                  </li>
                  <li>
                    <a id="test" href="#">
                      <span class="pull-right">Tests</span>
                    </a>
                  </li>
                  <li>
                      <span class="badge navbar-text pull-left">{{ len(tests) }}</span>
                  </li>
                </ul>
            </div>
        </nav>
        </div>
        <div class="row">
            <div id="warnings" class="warnings">
                {% for item in warnings %}
                <div class="list-group">
                    {% autoescape true %}
                        <a class="list-group-item" href="#">{{ item }}</a>
                    {% endautoescape %}
                </div>
                {% endfor %}
            </div>
            <div id="info" class="info">
                {% for item in infos %}
                <div class="list-group">
                    {% autoescape true %}
                        <a class="list-group-item" href="#">{{ item }}</a>
                    {% endautoescape %}
                </div>
                {% endfor %}
            </div>
            <div id="errors" class="errors">
                {% for item in errors %}
                <div class="list-group">
                    {% autoescape true %}
                        <a class="list-group-item" href="#">{{ item }}</a>
                    {% endautoescape %}
            </div>
                {% endfor %}
            </div>
            <div id="tests" class="tests">
                {% for item in tests %}
                <div class="list-group">
                    {% autoescape true %}
                        <a class="list-group-item" href="#">{{ item }}</a>
                    {% endautoescape %}
            </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>
</templates>
